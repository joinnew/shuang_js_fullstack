html
    width 100%

#footer
    margin-top 0
    width 100%
    background-color #f7f7f7
    border-top 1px solid #e4e4e4
    padding-top 30px
    padding-bottom 25px
    .footer-wrapper
        width 950px
        margin-left auto
        margin-right auto
        clear both
        overflow hidden
        color #bdbdbd
        text-align center
    .logo
        display inline-block
        vertical-align top
        font-size 32px
        line-height 1em
        color #9b9b9b
        margin-right 10px

.mui-icon
    font-family market-ui-iconfont !important 
    font-size 16px
    font-style normal
    -webkit-text-stroke-width .2px
    display inline-block
    vertical-align top
    text-align center
    &.logo:before
        content '\e603'
